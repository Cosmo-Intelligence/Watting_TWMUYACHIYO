<?xml version="1.0" encoding="UTF-8"?>
<Settings>

	<!-- 検査室名称 -->
	<!-- ※「,(カンマ)」は枠数に該当します。検査室パラメータ情報とリンクしています。 -->
	<KensaSituName>151,152,153</KensaSituName>

	<!-- 検査室パラメータ情報 -->
	<!--
		「,(カンマ)」は枠数に該当します。
		「|(パイプ)」を使用し、一枠に設定する「検査種別ID:検査室ID」を複数設定できます。
	 -->
	<KensaSituParam>1:CR001,1:CR002,1:CR003</KensaSituParam>

	<!-- 検査室未割当表示検査種別 -->
	<!-- ※検査室が未割当(Null)でも表示する検査種別IDを設定して下さい。 -->
	<MiwariateDispKensaType>1</MiwariateDispKensaType>
	
	<!-- 更新周期（秒） -->
	<ReloadSec>5</ReloadSec>

	<!-- 検中メッセージ -->
	<KenchuMsg>次の番号の方をお呼びしますので、それまでお待ちください。</KenchuMsg>

	<!-- 受済メッセージ -->
	<UkezumiMsg>以下の番号の方は、もうしばらくお待ちください。</UkezumiMsg>

	<!-- 受付番号の桁数 -->
	<ReceiptNumberDigit>4</ReceiptNumberDigit>

	<!-- 受付番号を補完する文字 -->
	<ComplementChar>0</ComplementChar>

	<!-- 本日検査日検索フラグ ※1:本日のみ  -->
	<TodayFlg>1</TodayFlg>
	
	<!-- テロップメッセージ -->
	<TelopMsg>撮影内容により順番が前後することが御ざいます。</TelopMsg>

</Settings>
